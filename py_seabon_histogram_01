import pandas as pd
import seaborn as sns
import matplotlib.pyplot as plt

# 读取 CSV 文件到 DataFrame
df = pd.read_csv('V_Counts.csv')

# 设置 Seaborn 样式
sns.set(style="whitegrid")

# 绘制直方图
plt.figure(figsize=(10, 6))
sns.barplot(x='column_name', y='column_value', data=df, palette='viridis')
plt.xlabel('Column Name')
plt.ylabel('Count of V')
plt.title('Count of V in Each Column')
plt.xticks(rotation=45)
plt.tight_layout()
plt.show()
